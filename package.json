{
  "name": "elwood-nuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "build:noIndex": "npm run build && echo 'User-agent: *\nDisallow: /' > dist/robots.txt",
